FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1000;  //  ReTheta = 1173.51-589.428*Tu+0.2196/Tu² = 584.302   with   Tu = 100*sqrt(2/3*k)/|U| = 1 // Rethetat = 1000 as in Müller et al


boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    Jet 
    {
        type            fixedValue;
        value           $internalField;
    }

    wallPreInlet
    {
        type            zeroGradient;            
    }

    upperWall
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //